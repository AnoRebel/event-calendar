{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "bunx eslint .",
    "lint:fix": "bunx eslint . --fix",
    "format": "bunx prettier --write .",
    "format:write": "bunx prettier --write \"**/*.{ts,vue,mdx}\" --cache",
    "format:check": "bunx prettier --check \"**/*.{ts,vue,mdx}\" --cache",
    "typecheck": "nuxt typecheck",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "deploy": "bash scripts/deploy.sh",
    "deploy:rollback": "bash scripts/deploy.sh rollback",
    "health-check": "bash scripts/deploy.sh health-check",
    "docker:build": "docker build -t event-calendar .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "performance:audit": "lighthouse http://localhost:3000 --output=html",
    "sw:generate": "workbox generateSW workbox-config.js"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/icon": "1.13.0",
    "@nuxt/image": "1.11.0",
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/vue-form": "^1.19.2",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@vue-dnd-kit/core": "^1.7.0",
    "@vueuse/core": "^13.6.0",
    "@vueuse/nuxt": "13.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "eslint": "^9.33.0",
    "lucide-vue-next": "^0.539.0",
    "nuxt": "^4.0.3",
    "reka-ui": "^2.4.1",
    "shadcn-nuxt": "2.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "uuid": "^11.1.0",
    "vue-sonner": "^2.0.2"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "@vercel/speed-insights",
    "esbuild",
    "sharp",
    "unrs-resolver",
    "vue-demi"
  ],
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.62",
    "@playwright/test": "^1.54.2",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.1",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "semver": "^7.7.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5",
    "workbox-cli": "^7.3.0"
  }
}
